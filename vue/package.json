    "autolinker": "^4.0.0",
    "axios": "^1.3.4",
    "babel-plugin-prismjs": "^2.1.0",
    "isomorphic-dompurify": "^1.9.0",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "tippy.js": "^6.3.7",
    "ts-md5": "^1.3.1",
    "typescript": "~5.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vue3-calendar-heatmap": "^2.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
