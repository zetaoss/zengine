    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^16.2.6",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
