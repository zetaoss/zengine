{
  "name": "ZetaExtension",
  "author": [
    "jmnote"
  ],
  "license-name": "Apache-2.0",
  "type": "other",
  "requires": {
    "MediaWiki": ">= 1.39.0",
    "platform": {
      "php": ">= 7.2"
    }
  },
  "config": {
    "ZetaAllowHosts": {
      "value": []
    }
  },
  "AutoloadNamespaces": {
    "ZetaExtension\\": "includes/"
  },
  "AuthManagerAutoConfig": {
    "primaryauth": {
      "OTPAuthProvider": {
        "class": "ZetaExtension\\Auth\\OTPAuthProvider"
      }
    }
  },
  "ExtensionMessagesFiles": {
    "TagMagic": "magic/TagMagic.php"
  },
  "Hooks": {
    "InternalParseBeforeLinks": "ZetaExtension\\Toc\\Toc::onInternalParseBeforeLinks",
    "PageSaveComplete": "ZetaExtension\\Binder\\BinderHooks::onPageSaveComplete",
    "PageDeleteComplete": "ZetaExtension\\Binder\\BinderHooks::onPageDeleteComplete",
    "PageUndeleteComplete": "ZetaExtension\\Binder\\BinderHooks::onPageUndeleteComplete",
    "ParserFirstCallInit": [
      "ZetaExtension\\Map\\Map::onParserFirstCallInit",
      "ZetaExtension\\Tag\\Tag::onParserFirstCallInit"
    ],
    "PostLoginRedirect": "ZetaExtension\\Auth\\Hooks::onPostLoginRedirect",
    "SpecialPageBeforeExecute": "ZetaExtension\\Auth\\Hooks::onSpecialPageBeforeExecute",
    "UserLogoutComplete": "ZetaExtension\\Auth\\Hooks::onUserLogoutComplete"
  },
  "RestRoutes": [
    {
      "path": "/auth/{code}",
      "method": "GET",
      "class": "ZetaExtension\\Auth\\Rest"
    },
    {
      "path": "/binder",
      "method": "GET",
      "class": "ZetaExtension\\Binder\\RestBinder"
    },
    {
      "path": "/binder/{pageid}",
      "method": "GET",
      "class": "ZetaExtension\\Binder\\RestBinder"
    }
  ],
  "ResourceModules": {
    "map1": {
      "scripts": [
        "modules/map1.js"
      ]
    },
    "map2": {
      "scripts": [
        "modules/map2.js"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "ZetaExtension"
  },
  "manifest_version": 2
}